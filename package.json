{
  "name": "sitesurvey",
  "version": "0.0.1",
  "description": "Site Survey tool",
  "main": "webpack.config.babel.js",
  "scripts": {
    "dev": "cross-env BABEL_DISABLE_CACHE=1 node -r babel-register -r dotenv/config index.js",
    "build": "npm run build:client && npm run build:thumb-service && npm run build:api && npm run build:gdrivesync",
    "build:api": "cross-env BABEL_DISABLE_CACHE=1 webpack --config=api/webpack.config.babel.js",
    "build:client": "cross-env BABEL_DISABLE_CACHE=1 webpack --config=client/webpack.config.babel.js --env=production",
    "build:thumb-service": "cross-env BABEL_DISABLE_CACHE=1 webpack --config=thumb-service/webpack.config.babel.js",
    "build:gdrivesync": "cross-env BABEL_DISABLE_CACHE=1 webpack --config=gdrivesync/webpack.config.babel.js",
    "build:photo-importer": "gulp --gulpfile photo-importer/gulpfile.babel.js",
    "knex": "node -r babel-register -r dotenv/config node_modules/.bin/knex",
    "db:setup": "npm run db:movemigrations && npm run db:migrate && npm run db:seed && npm run db:movemigrationsback && npm run db:migrate",
    "db:movemigrations": "mv migrations/*_moveUserData.js .",
    "db:movemigrationsback": "mv *_moveUserData.js migrations/",
    "db:seed": "npm run knex -- seed:run",
    "db:migrate": "npm run knex -- migrate:latest",
    "db:rollback": "npm run knex -- migrate:rollback",
    "db:check-migrate": "npm run db:migrate && npm run db:rollback && npm run db:migrate",
    "test": "jest --forceExit",
    "test:code": "jest -t='code' --forceExit",
    "test:build": "jest -t='post build' --forceExit",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint src/**/*.js",
    "lint:css": "stylelint src/**/*.css",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "private": true,
  "author": "Scott McDonald <scott.mcdonald@arup.com>",
