{
  "extends":[
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "prettier"
  ],
  "parser":"babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": ["error", {"singleQuote": true, "semi": false}]
  }
}
